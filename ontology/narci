     narci:onto owl:Nothing;
     narci:relatedTo narci:anesthetics_type;
     skos:definition """
                     Flow rate of the anaesthetic drug injected into 
                     animal.
                     """@en .

narci:anesthetics_oxygen a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "anesthetics_oxygen";
