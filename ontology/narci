# NARCI Ontology
<http://www.purl.org/narci/schema#> a owl:Ontology ;
     dcterm:title "narci" ;
     dcterm:modified "2017-10-25"^^<http://www.w3.org/2001/XMLSchema#date>;
     dcterm:description """
                            (c) 2017 University of Amsterdam
                                     Forschungszentrum Juelich GmbH
                                     The German Neuroinformatics Node (G-Node)
                                     Le Centre National de la Recherche Scientifique (CRNS)

                            narci is an ontology for calcium imaging.
                            Developed under the auspices of 
                            European Union
                            Human Brain Project (HBP)
                            Work package SP 5.7.2 named NAR.
                            NAR stands for Neural Activity Resource.
                            narci stands for NAR Calcium Imaging.
                 
                         """@en ;
     dcterm:creator narci:MS;
     dcterm:contributor narci:GTM;
     dcterm:contributor narci:XT;
     dcterm:contributor narci:JSM;
     dcterm:contributor narci:CSL;
     dcterm:contributor narci:MD;
     dcterm:contributor narci:APD;
     dcterm:contributor narci:TW;
     dcterm:contributor narci:SG;
     dcterm:contributor narci:CMAP .

narci:Metadata a rdfs:Class;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "Metadata";
     skos:definition """
                     Calcium imaging metadata root class.
                     """@en .

narci:Experiment a rdfs:Class;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "Experiment";
     rdfs:subClassOf narci:Metadata;
     skos:definition """
                     Calcium imaging experimental details in a given 
                     session. Stimulus, recording and in-vivo 
                     biological details.
                     """@en .

narci:Scanner a rdfs:Class;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "Scanner";
     rdfs:subClassOf narci:Metadata;
     skos:definition """
                     Calcium imaging scanning technical details in a given
                     experiment. 
                     """@en .

narci:Cell a rdfs:Class;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "Cell";
     rdfs:subClassOf narci:Metadata;
     skos:definition """
                     Calcium imaging imaged cell technical details.
                     """@en .

narci:experiment a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "experiment";
     rdf:domain narci:Metadata;
     rdf:range narci:Experiment;
     rdfs:type owl:DatatypeProperty;
     skos:definition """
                     Calcium imaging experimental details in a given
                     session. Stimulus, recording and in-vivo
                     biological details. (a narci:Experiment)
                     """@en .

narci:scanner a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "scanner";
     rdf:domain narci:Metadata;
     rdf:range narci:Scanner;
     rdfs:type owl:DatatypeProperty;
     skos:definition """
                     Calcium imaging scanning technical details in a given
                     experiment (a narci:Scanner).
                     """@en .

narci:cell a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "cell";
     rdf:domain narci:Metadata;
     rdf:range narci:Cell;
     rdfs:type owl:DatatypeProperty;
     skos:definition """
                     Calcium imaging imaged cell technical details.
                     (a narci:Cell)
                     """@en .

narci:Properties a rdfs:Class;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "Properties";
     rdfs:subClassOf narci:Metadata;
     rdf:type owl:DatatypeProperty;
     skos:definition """
                     Calcium imaging additional properties
                     that can be used as a property for
                     properties.
                     """@en .

narci:reference a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "reference";
     rdfs:range xsd:string;
     rdf:domain narci:Properties;
     rdf:type owl:DatatypeProperty;
     skos:definition """
                     A reference to concept or property definition.
                     """@en .

narci:unit a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "unit";
     rdf:range xsd:string;
     rdf:domain narci:Properties;
     rdf:type owl:DatatypeProperty;
     skos:definition """
                     A unit for a physical measurement.
                     """@en .

narci:onto a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "onto" ;
     rdfs:range xsd:string;
     rdf:domain narci:Properties;
     rdf:type owl:DatatypeProperty;
     skos:definition """
                     Reference to other ontology on the given 
                     individual.
                     """@en .

narci:relatedTo a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "relatedTo";
     rdfs:range xsd:string;
     rdf:domain narci:Properties;
     rdf:type owl:ObjectProperty;
     skos:definition """
                     Two individuals are conceptually related to
                     each other. 
                     """@en .


narci:animal_identification a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "animal_identification";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "https://en.wikipedia.org/wiki/Animal_identification";
     narci:reference "https://en.wikipedia.org/wiki/Microchip_implant_(animal)";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo owl:Nothing;
     skos:definition """
                     Internal identification of the animal that 
                     experiment is performed on. Usually, 
                     animal identification, using a means of 
                     marking is a process done to identify and 
                     track specific animals.
                     """@en .

narci:imaging_date a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "imaging_date";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:date;
     rdf:type owl:DatatypeProperty;
     narci:reference owl:Nothing;
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo owl:Nothing;
     skos:definition """
                     A date when experiment(s) are performed 
                     formatted with yyyy-mm-dd.
                     """@en .

narci:imaging_session a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "imaging_session";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "Allen Brain Observatory";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_date;
     skos:definition """
                     A tag for continuous time frame where calcium 
                     imaging acquisition is performed.
                     """@en .

narci:imaging_session_id a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "imaging_session_id";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:nonNegativeInteger;
     rdf:type owl:DatatypeProperty;
     narci:reference "Pennarz Lab";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_session;
     skos:definition """
                     The session identification for the performed 
                     imaging experiment.
                     """@en .

narci:image_acquisition_system a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "image_acquisition_system";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "https://en.wikipedia.org/wiki/Medical_imaging";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo owl:Nothing;
     skos:definition """
                     A system to perform medical imaging, that is 
                     the technique and process of creating visual 
                     representations of the interior of a body for 
                     clinical analysis and medical intervention, 
                     as well as visual representation of the 
                     function of some organs or tissues 
                     (physiology).
                     """@en .

narci:imaging_hardware owl:sameAs narci:image_acquisition_system .
narci:imaging_system owl:sameAs narci:image_acquisition_system .

narci:imaging_calcium_device a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "imaging_calcium_device";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "Grienberger-Konnerth, Neuron 73, 862-885 (2012), DOI:10.1016/j.neuron.2012.02.011";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:image_acquisition_system;
     skos:definition """
                     A device that provides 3D brain tissue 
                     imaging capabilities, including confocal 
                     and two-photon microscopy.
                     """@en .

narci:two_photon_imaging_instrument owl:sameAs narci:imaging_calcium_device .

narci:perception a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "perception";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "https://en.wikipedia.org/wiki/Perception, Schacter, Daniel (2011). Psychology. Worth Publishers.";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo owl:Nothing;
     skos:definition """
                     Perception (from the Latin perceptio) is the organization, 
                     identification, and interpretation of sensory information 
                     in order to represent and understand the presented 
                     information, or the environment. All perception involves 
                     signals that go through the nervous system, which in turn 
                     result from physical or chemical stimulation of the 
                     sensory system.
                     """@en .

narci:stimulus a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "stimulus";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "https://en.wikipedia.org/wiki/Stimulus_(physiology)";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:perception;
     skos:definition """
                     A stimulus (plural stimuli) is a detectable change in the 
                     internal or external environment.
                     """@en .

narci:stim_protocol owl:sameAs rdfs:Property .
 
narci:sensory_neurons a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "sensory_neurons";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "https://en.wikipedia.org/wiki/Sensory_neuron";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:stimulus;
     skos:definition """
                     Sensory neurons also known as afferent neurons are neurons 
                     that convert a specific type of stimulus, via their 
                     receptors, into action potentials or graded potentials.
                     """@en .

narci:visual_stimuli a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "visual_stimuli";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "Allen Brain Observatory";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:stimulus;
     narci:relatedTo narci:sensory_neurons;
     skos:definition """
                     An external light sources able to emit modulated light, 
                     that generates neuronal response.
                     """@en .

narci:visual_cortex a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "visual_cortex";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "https://en.wikipedia.org/wiki/Visual_cortex";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:visual_stimuli;
     skos:definition """
                     An external light sources able to emit modulated light, 
                     that generates neuronal response.
                     """@en .


narci:imaging_depth a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#> ;
     rdfs:label "imaging_depth";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:nonNegativeInteger;
     rdf:type owl:DatatypeProperty;
     narci:reference "Pennarz Lab";
     narci:unit "micrometer";
     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_calcium_device;
     narci:relatedTo narci:visual_cortex;
     skos:definition """
                     Physical depth of image plane from
                     the surface of visual cortex. 
                     """@en .

narci:cortical_depth owl:sameAs narci:imaging_depth .

narci:anesthetics_type a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "anesthetics_type";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "Allen Brain Observatory, https://en.wikipedia.org/wiki/Anesthetic";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     skos:definition """
                     Type of anesthesia administred during 
                     animal preperation. Such as Local,
                     General or any other specific type.
                     """@en .

narci:anesthetics_machine a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "anesthetics_machine";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "Allen Brain Observatory, https://en.wikipedia.org/wiki/Anesthetic";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:anesthetics_type;
     skos:definition """
                     The supporting machine or system that helps in 
                     administering anesthesia.
                     """@en .

narci:anesthetics_drug a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
