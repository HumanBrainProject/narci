     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_session;
     skos:definition """
                     A wall clock time where the imaging  
                     session ends. (hh:mm:ss)
                     """@en .

narci:imaging_session_duration a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "imaging_session_duration";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:double;
     rdf:type owl:DatatypeProperty;
     narci:reference "Pennartz Lab";
     narci:unit "s";
     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_session;
     skos:definition """
                     The total wall clock time of the
                     imaging session.
                     """@en .

narci:neuronal_calcium_sensors a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "neuronal_calcium_sensors";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:string;
     rdf:type owl:DatatypeProperty;
     narci:reference "Allen Brain Observatory;https://en.wikipedia.org/wiki/Calcium_imaging#Chemical_indicators; Burgoyne, Nat Rev Neurosci. 2007 March ; 8(3): 182–193. doi:10.1038/nrn2093 ; Badura et. al. Neurophotonics, 1(2), 025008 (2014). doi:10.1117/1.NPh.1.2.025008, https://www.janelia.org/gcamp6-calcium-indicators-0";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_calcium_device;
     narci:relatedTo narci:visual_cortex;
     skos:definition """
                     Fluorescent molecules, usually protein, that can 
                     respond to the binding of Ca2+ ions by changing 
                     their fluorescence properties. This usually manifest 
                     as genetically encoded into transgenetic animal. 
                     Plasmids: CMV-GCaMP6s, CMV-GCaMP6m, CMV-GCaMP6f. 
                     Virusus: AAV1.Syn.GCaMP6f.WPRE.SV40, 
                     AAV1.Syn.GCaMP6s.WPRE.SV40, 
                     AAV1.Syn.GCaMP6m.WPRE.SV40, AAV5.Syn.GCaMP6m.WPRE.SV40, 
                     AAV5.Syn.GCaMP6f.WPRE.SV40, AAV5.Syn.GCaMP6s.WPRE.SV40, 
                     AAV9.Syn.GCaMP6f.WPRE.SV40, AAV9.Syn.GCaMP6m.WPRE.SV40, 
                     AAV9.Syn.GCaMP6s.WPRE.SV40, AAV1.CAG.GCaMP6s.WPRE.SV40, 
                     AAV5.CAG.GCaMP6s.WPRE.SV40, AAV9.CAG.GCaMP6s.WPRE.SV40, 
                     AAV1.CAG.Flex.GCaMP6f.WPRE.SV40, 
                     AAV1.CAG.Flex.GCaMP6m.WPRE.SV40, 
                     AAV1.CAG.Flex.GCaMP6s.WPRE.SV40, 
                     AAV1.Syn.Flex.GCaMP6f.WPRE.SV40, 
                     AAV1.Syn.Flex.GCaMP6m.WPRE.SV40, 
                     AAV1.Syn.Flex.GCaMP6s.WPRE.SV40, 
                     AAV5.CAG.Flex.GCaMP6f.WPRE.SV40, 
                     AAV5.CAG.Flex.GCaMP6m.WPRE.SV40, 
                     AAV5.CAG.Flex.GCaMP6s.WPRE.SV40, 
