                     """@en .

narci:imaging_session_start a rdfs:Property;
     rdfs:isDefinedBy <http://www.purl.org/narci/schema#>;
     rdfs:label "imaging_session_start";
     rdfs:domain narci:Experiment;
     rdfs:range xsd:date;
     rdf:type owl:DatatypeProperty;
     narci:reference "Pennartz Lab";
     narci:unit owl:Nothing;
     narci:onto owl:Nothing;
     narci:relatedTo narci:imaging_session;
     skos:definition """
                     A wall clock time where the imaging
                     session starts. (hh:mm:ss)
                     """@en .

